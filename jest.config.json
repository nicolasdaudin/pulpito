{
  "verbose": true,
  "setupFiles": ["dotenv/config"],
  "collectCoverageFrom": [
    "src/**/*.ts",
    "!public/**/*.ts",
    "!src/*.ts",
    "!coverage/**/*.ts",
    "!**/node_modules/**",
    "!dev-data/**/*.ts"
  ],
  "preset": "ts-jest",
  "testEnvironment": "node",
  "modulePathIgnorePatterns": ["<rootDir>/build/"]
}
